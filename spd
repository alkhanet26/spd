#!/bin/bash
#This Script by ALKHANET (http://github.com/alkhanet26)

up="$1"

if [ "$1" = "up" ]; then
echo -e "Downloading Binaries"
wget --no-check-certificate "https://raw.githubusercontent.com/alkhanet26/spd/main/speedtest" -O /usr/bin/speedtest && chmod +x /usr/bin/speedtest
else
echo -e "Menguji Kecepatan Internet"
speedtest --progress=no
fi
